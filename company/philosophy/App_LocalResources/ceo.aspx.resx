  <data name="ceosign.text" xml:space="preserve">
    <value>&lt;div class="sign"&gt;
					&lt;img class="img_logo" src="/ko/common/img/ko/page/company/pop_ceo_logo.png" alt="LS ELECTRIC"&gt;&lt;br /&gt;
					CEO &amp;amp; 회장&lt;br /&gt;
